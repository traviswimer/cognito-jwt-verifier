		"@types/jwk-to-pem": "^2.0.1",
		"@types/node": "^18.0.5",
		"jest": "^28.1.3",
		"ts-jest": "^28.0.6",
		"ts-node": "^10.9.1",
		"tsup": "^6.1.3",
		"typescript": "^4.7.4"
